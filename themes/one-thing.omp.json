{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#A9E5BB",
          "foreground": "#011627",
          "leading_diamond": "\uE0B6",
          "properties": {
            "macos": "\uf179 ",
            "ubuntu": "\uf31b ",
            "windows": "\ue62a "
          },
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "trailing_diamond": "<transparent,#A9E5BB>\ue0b2</>",
          "type": "os"
        },
        {
          "background": "#A9E5BB",
          "foreground": "#011627",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\uf489 {{ .Name }}",
          "trailing_diamond": "<transparent,#A9E5BB>\ue0b2</>",
          "type": "shell"
        },
        {
          "background": "#e0aaff",
          "foreground": "#3D002E",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue266 MEM: {{ round .PhysicalPercentUsed .Precision }}% | {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB \ue266 ",
          "trailing_diamond": "<transparent,#e0aaff>\ue0b2</>",
          "type": "sysinfo"
        },
        {
          "background": "#fbc4ab",
          "foreground": "#4f000b",
          "leading_diamond": "\ue0b2",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "trailing_diamond": "\uE0B4",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#ff97b7",
          "foreground": "#011627",
          "leading_diamond": "\uE0B6",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "diamond",
          "template": "{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\uE0B4",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text",
          "foreground": "#adb5bd"
        },
        {
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": " \u2665 {{ .CurrentDate | date .Format }} |",
          "type": "time",
          "foreground": "#000000"
        },
        {
          "style": "plain",
          "template": " \uf292 ",
          "type": "root",
          "foreground": "#adb5bd"
        },
        {
          "properties": {
            "folder_icon": "\uf07b ",
            "folder_separator_icon": " \uf061 ",
            "home_icon": "\ueb06 "
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path",
          "foreground": "#adb5bd"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u2570\u2500 ",
          "type": "status",
          "foreground": "#adb5bd"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "#e0aaff",
    "foreground": "#3c096c",
    "template": "<#e0aaff,transparent>\uE0B6</>\u2665 {{ .Shell }}<#e0aaff,transparent>\uE0B4</>",
    "newline": true
  },
  "version": 3
}
